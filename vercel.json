{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                },
                {
                    "key": "X-Robots-Tag",
                    "value": "index, follow"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src *; img-src * data:; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline';"
                }
            ]
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/home",
            "destination": "/",
            "permanent": true
        }
    ]
}